{% extends "editorial_content/layout.html.twig" %}

{% block bodyClass %}articulo{% endblock %}

{% block bodyId %}{{ content.section.slug }}{% endblock %}

{% block content %}
<main class="contenedor clear" itemscope itemtype="http://schema.org/NewsArticle">
    <div class="columna-ppal">
        {{ htmlTitle | raw }}
    </div>
    {% if content.subtitles is defined and content.subtitles | length > 0 %}
    <div class="columna-ppal subtitulos">
        {% for subtitle in content.subtitles %}
        <p itemprop="alternativeHeadline">{{ subtitle }}</p>
        {% endfor %}
    </div>
    {% endif %}
    <div class="columna-ppal">
        <ul class="herramientas clear">
            <li class="bloque">
                {% for author in authors %}
                <a href="" class="firma"><span itemprop="author">{{ author.firstName }} {{ author.lastName }}</span></a>
                {% endfor %}
                <time class="fecha" itemprop="datePublished" datetime="{{ content.publishedDT | date('Y-m-d') }}">{{ content.publishedDT | date('d') }} de {{ months[(content.publishedDT | date('n')) - 1] }} de {{ content.publishedDT | date('Y') }}</time>
            </li>
            <li class="bloque">
                <span class="boton-comentarios">24<span class="punta"><img src="img/punta-comentarios.png" alt=""></span></span>
            </li>
            <li class="bloque">
                {{ socialMediaMacros.editorialContentSocialSharingButtons(content.id, app.request.getSchemeAndHttpHost()~app.request.getBaseURL()~content.urls.0.url, content.title) }}
            </li>
        </ul>
        {% if primaryImage is defined and primaryImage != null and primaryImage.path | length > 0 %}
        <figure class="foto" itemprop="image" itemscope itemtype="http://schema.org/ImageObject">
            <img src="{{ multimedia_domain ~ relative_images_url ~ primaryImage.path | optimized('article_primary') }}" alt="{{ primaryImage.description }}" itemprop="url">
            {% if (primaryImage.title != null and primaryImage.title | length > 0) or (primaryImage.author != null and primaryImage.author | length > 0) %}
            <figcaption itemprop="name">{% if primaryImage.title != null and primaryImage.title | length > 0 %}<strong>{{ primaryImage.title }}</strong> {% endif %}{% if primaryImage.description is defined and primaryImage.description|length > 0 %}{{ primaryImage.description }} {% endif %}{% if primaryImage.author != null and primaryImage.author | length > 0 %}<span class="firma">{{ primaryImage.author }}</span>{% endif %}</figcaption>
            {% endif %}
        </figure>
        {% endif %}
        <div class="cuerpo-articulo" itemprop="articleBody">
            {% if content.intro is defined and content.intro|length > 0 %}
            <p class="entradilla-articulo">{{ content.intro }}</p>
            {% endif %}
            {% if content.text | length > 0 %}
            {{ content | formatedTextContent(advertisingMacros.roba('resto')) | raw }}
            {% endif %}
            <aside class="guardado-en">
                <h1>Guardado en</h1>
                <ul>
                    {% for category in content.categories %}
                    <li><a href="{{ app.request.getSchemeAndHttpHost() ~ app.request.baseUrl ~ category.url }}">{% if category.id == content.section.id %}<strong itemprop="articleSection">{{ category.name }}</strong>{% else %}<span itemprop="articleSection">{{ category.name }}</span>{% endif %}</a></li>
                    {% endfor %}
                    {% for tag in content.tags %}
                    <li><a href="{{ app.request.getSchemeAndHttpHost() ~ app.request.baseUrl ~ tag.url }}"><span itemprop="articleTag">{{ tag.name }}</span></a></li>
                    {% endfor %}
                </ul>
            </aside>
        </div>
        {{ render(controller('BloqModulesBundle:EditorialContent:canInterestYou', {'title': 'Te puede interesar', 'limit': 4, 'content': content, 'excludedContents': counters.usedContents })) }}
        <div class="robapaginas">
        {{ advertisingMacros.roba('resto') }}
        </div>
        <aside class="en-redes">
            <h1>En redes</h1>
        </aside>
    </div>
    <div class="columna-ppal">
        <section class="comentarios">
            <h1 class="titulo-seccion">Comentarios</h1>
            <div id="disqus_thread"></div>
            <script>
                /**
                *  RECOMMENDED CONFIGURATION VARIABLES: EDIT AND UNCOMMENT THE SECTION BELOW TO INSERT DYNAMIC VALUES FROM YOUR PLATFORM OR CMS.
                *  LEARN WHY DEFINING THESE VARIABLES IS IMPORTANT: https://disqus.com/admin/universalcode/#configuration-variables
                */
                var disqus_config = function () {
                    this.page.url = "{{ path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) }}";  // Replace PAGE_URL with your page's canonical URL variable
                    this.page.identifier = "{{ disqus_editorial_content_id_prefix }}{{ content.id }}"; // Replace PAGE_IDENTIFIER with your page's unique identifier variable
                };
                (function() {  // DON'T EDIT BELOW THIS LINE
                    var d = document, s = d.createElement('script');
        
                    s.src = '//{{ disqus_src_domain_prefix }}.disqus.com/embed.js';
 
                    s.setAttribute('data-timestamp', +new Date());
                    (d.head || d.body).appendChild(s);
                })();
            </script>
            <noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript" rel="nofollow">comments powered by Disqus.</a></noscript>
        </section>
